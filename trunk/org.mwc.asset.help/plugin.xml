<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         id="org.mwc.asset.help"
         point="org.eclipse.help.toc">
      <toc
            file="html/legacy/toc2.xml"
            primary="true">
      </toc>
   </extension>
   
   <extension point="org.eclipse.ui.actionSets">
     <actionSet label="Cheat Sheets" 
        visible="true" id="org.eclipse.ui.cheatsheets.actionSet">
			 <action label="ASSET Cheat Sheets" 
			     class="org.eclipse.ui.cheatsheets.CheatSheetExtensionFactory:helpMenuAction"
					menubarPath="help/helpEnd"
					id="org.eclipse.ui.cheatsheets.actions.CheatSheetHelpMenuAction">
			</action>
		</actionSet>
	</extension>
   
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="HelpContexts.xml"  plugin="org.mwc.asset.help"/>
   </extension>
</plugin>
